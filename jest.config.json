{
  "testEnvironment": "jsdom",
  "collectCoverageFrom": ["./src/*.ts", "./__tests__/*.tsx", "./__tests__/*.ts"],
  "coverageDirectory": "./coverage",
  "coverageReporters": ["text", "html"],
  "setupFilesAfterEnv": ["./jestSetup.ts"],
  "projects": ["<rootDir>/jest.config.json"],
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  }
}
